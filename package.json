{
  "name": "MemoryKing",
  "productName": "Memory King",
  "version": "0.1.0",
  "description": "Memory training app.",
  "main": "main.js",
  "scripts": {
    "start": "webpack && electron .",
    "watch": "webpack --watch",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "devstart": "cross-env NODE_ENV=development electron .",
    "server": "babel-node server.js",
    "build": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "package": "webpack --config webpack.config.production.js && electron-packager . MemoryKing --all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Breta01/MemoryKing"
  },
  "keywords": [
    "Memory King",
    "Memory",
    "King",
    "training"
  ],
  "author": "Břetislav Hájek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Breta01/MemoryKing/issues"
  },
  "homepage": "https://github.com/Breta01/MemoryKing#readme",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "electron-prebuilt": "^1.3.2",
    "menubar": "^5.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-register": "^6.11.6",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "debounce": "^1.0.0",
    "devtron": "^1.3.0",
    "electron-packager": "^7.5.1",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "mocha": "^3.0.2",
    "moment": "^2.14.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.0",
    "sass-loader": "^4.0.0",
    "spectron": "^3.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
